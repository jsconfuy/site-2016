    block css

    block head

  body

    block top

    block header
      nav(role="navigation").navbar.navbar-default.navbar-fixed-top
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/')
            img(src='/images/base/small-logo.svg', alt='JSConf Uruguay')
        .collapse.navbar-collapse
          ul.nav.navbar-nav
            // li(class=(section == 'cft' ? 'active' : null))
            //   a(href='/talks') Call for Talks
            // li(class=(section == 'cfw' ? 'active' : null))
            //   a(href='/workshops') Call for Workshops
            li
              a(href='/#speakers') Speakers
            //li(class=(section == 'schedule' ? 'active' : null))
              a(href='/schedule') Schedule
            li.nav-logo
              a(href='/')
                img(src="/images/base/small-logo.svg")
            li
              a(href='/#sponsors') Sponsors
            li(class=(section == 'venue' ? 'active' : null))
              a(href='/venue') Venue
            //li(class=(section == 'hotels' ? 'active' : null))
              a(href='/hotels') Hotels
            // li(class=(section == 'coc' ? 'active' : null))
            //   a(href='/code-of-conduct') Code of Conduct
            li(class=(section == 'blog' ? 'active' : null))
              a(href='/blog') Blog
            li
              a(href='http://2015.jsconf.uy/') Last Year
            //li
            //  a(href='javascript:void(0);', data-buy) Buy Ticket!

    block main

      block content

    block modals
      #buy.modal.fade(tabindex='-1', role='dialog', aria-hidden='true', data-order='#{buy.order || ""}', data-ticket='#{buy.ticket || ""}', data-discount='#{buy.discount || ""}')
        .modal-dialog
          .modal-content
            .modal-body
              .intro
                button(type='button', class='close', data-dismiss='modal', aria-label='Close')
                  span(aria-hidden='true') &times;
                img(src='images/home/buy-ticket.svg')
              .indicators
                .indicator.indicator-select
                  i(class='fa fa-shopping-cart')
                  span Select
                .indicator.indicator-payment
                  i(class='fa fa-credit-card')
                  span Payment
                .indicator.indicator-assign
                  i(class='fa fa-ticket')
                  span Assign
              .steps
                .step.step-select
                  .loading
                    .loader
                  .description
                    span A Ticket includes
                    ul
                      li
                        i(class='fa fa-check')
                        |  Two day access Lanyard
                      li
                        i(class='fa fa-check')
                        |  Lunch both days
                      li
                        i(class='fa fa-check')
                        |  Breakfast and coffee-breaks
                      li
                        i(class='fa fa-check')
                        |  A cool T-Shirt
                  .discount
                    .code
                  .soldout
                    | Titcket sale has not started yet.
                    // We're sold out.
                  .tickets
                    ul
                  .note
                    | To participate in the conference you must adhere to
                    a(href='/code-of-conduct', target='_blank') our code of conduct
                    | .
                    br
                    | Any questions you may have email us at
                    a(href='mailto:hola@jsconf.uy', target='_blank') hola@jsconf.uy
                .step.step-payment
                  .loading
                    .loader
                  .invoice
                    .detail
                    .pay
                      button Pay
                  .important
                    | At this point your tickets has been reserved. You have #{ticketReservation} minutes to complete your payment or these will be released.
                  .note
                    | A popup will be open to process with the checkout.
                    br
                    | Any questions please contact
                    a(href='mailto:hola@jsconf.uy', target='_blank') hola@jsconf.uy
                .step.step-assign
                  .loading
                    .loader
                  .attendees
                    ul
                  .thanks
                    | Thank you!
                  .save
                    button Save & Close
                  .note You can complete this step later.
    block footer
      footer
        .footer
          .venue-info
            .venue
              h1 Antel Auditorium
              h2
                |15
                span.lowercase th 
                |& 16
                span.lowercase th 
                |April 2016
            .info
              h3 Additional Information
              | Please feel free to contact us at
              a(href='mailto:hola@jsconf.uy', target='_blank') hola@jsconf.uy
              | if you need assistance with flight, hotel bookings and with general rules about getting to the country, we are glad to help.
              h3 Stay In Touch
              form.data(action="//jsconf.us7.list-manage.com/subscribe/post?u=522df5bbc3b97d9e5f5c16fe0&id=7f258c3ebb", method="post")
                .group
                  input(name='EMAIL', type='text', required="required")
                  .highlight
                  .bar
                  label Email address
              .social
                a(href='//twitter.com/jsconfuy')
                  img(src='/images/base/twitter.svg')
                //| -
                //a(href='//www.facebook.com/JSConfUY') Facebook
                //| -
                a(href='//www.youtube.com/user/jsconfuy')
                  img(src='/images/base/youtube.svg')
          #map.map
        .copy
          .box
            a(href='//meetup.com/mvd-js') Open Web Uruguay
            a(href='/code-of-conduct') Code of Conduct

    script(type='text/javascript').
      window.tickets = {
        reservation: #{ticketReservation}
      };

    script(src='//maps.googleapis.com/maps/api/js')
    script(src='/js/lib/jquery/jquery-2.1.1.min.js')
    script(src='/js/lib/bootstrap/bootstrap-3.2.0.min.js')
    script(src='/js/lib/jquery.bootstrap-autohidingnavbar.js')
    script(src='/js/base.js')

    script(type='text/javascript').
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-40076270-1']);
      _gaq.push(['_trackPageview']);
      (function() {
       var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
       ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
       var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();

    if user && user.canAccessKeystone
      script(src='/keystone/js/content/editor.js')

    block js
